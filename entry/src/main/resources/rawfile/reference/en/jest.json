    {
      "id": "s6-28",
      "sectionId": "s6",
      "title": "Basic",
      "kind": "code",
      "lang": "js",
      "code": "expect(value).not.toBe(value);\nexpect(value).toEqual(value);\nexpect(value).toBeTruthy();"
    },
    {
      "id": "s6-29",
      "sectionId": "s6",
      "title": "Basic",
      "kind": "text",
      "body": "> Note: `toEqual` performs deep equality."
    },
    {
      "id": "s6-30",
      "sectionId": "s6",
      "title": "Snapshots",
      "kind": "code",
      "lang": "js",
      "code": "expect(value).toMatchSnapshot();\nexpect(value).toMatchInlineSnapshot();"
    },
    {
      "id": "s6-31",
      "sectionId": "s6",
      "title": "Snapshots",
      "kind": "text",
      "body": "> Inline snapshots require Prettier."
    },
    {
      "id": "s6-32",
      "sectionId": "s6",
      "title": "Errors",
      "kind": "code",
      "lang": "js",
      "code": "expect(fn).toThrow(error);\nexpect(fn).toThrowErrorMatchingSnapshot();"
    },
    {
      "id": "s6-33",
      "sectionId": "s6",
      "title": "Booleans",
      "kind": "code",
      "lang": "js",
      "code": "expect(value).toBeFalsy();\nexpect(value).toBeNull();\nexpect(value).toBeTruthy();\nexpect(value).toBeUndefined();\nexpect(value).toBeDefined();"
    },
    {
      "id": "s6-34",
      "sectionId": "s6",
      "title": "Numbers",
      "kind": "code",
      "lang": "js",
      "code": "expect(value).toBeCloseTo(number, digits);\nexpect(value).toBeGreaterThan(number);\nexpect(value).toBeLessThanOrEqual(number);"
    },
    {
      "id": "s6-35",
      "sectionId": "s6",
      "title": "Objects",
      "kind": "code",
      "lang": "js",
      "code": "expect(value).toBeInstanceOf(Class);\nexpect(value).toMatchObject(obj);\nexpect(value).toHaveProperty('key', value);"
    },
    {
      "id": "s6-36",
      "sectionId": "s6",
      "title": "Arrays/Strings",
      "kind": "code",
      "lang": "js",
      "code": "expect(value).toContain(item);\nexpect(value).toHaveLength(number);\nexpect(value).toMatch(/pattern/);"
    },
    {
      "id": "s6-37",
      "sectionId": "s6",
      "title": "Custom Matchers",
      "kind": "code",
      "lang": "js",
      "code": "expect.extend(customMatchers);\nexpect.any(Constructor);\nexpect.assertions(1);"
    },
    {
      "id": "s6-38",
      "sectionId": "s6",
      "title": "Custom Matchers",
      "kind": "text",
