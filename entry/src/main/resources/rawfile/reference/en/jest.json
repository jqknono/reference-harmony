      "id": "s10-50",
      "sectionId": "s10",
      "title": "Creating Mocks",
      "kind": "code",
      "lang": "js",
      "code": "const fn = jest.fn();\nconst squared = jest.fn((n) => n * n);"
    },
    {
      "id": "s10-51",
      "sectionId": "s10",
      "title": "Assertions",
      "kind": "code",
      "lang": "js",
      "code": "expect(fn).toHaveBeenCalled();\nexpect(fn).toHaveBeenCalledTimes(2);\nexpect(fn).toHaveBeenCalledWith(arg1, arg2);"
    },
    {
      "id": "s10-52",
      "sectionId": "s10",
      "title": "Flexible Matchers",
      "kind": "code",
      "lang": "js",
      "code": "expect(fn).toHaveBeenCalledWith(expect.any(String));\nexpect(fn).toHaveBeenCalledWith(expect.arrayContaining([1, 2]));"
    },
    {
      "id": "s10-53",
      "sectionId": "s10",
      "title": "Instances",
      "kind": "code",
      "lang": "js",
      "code": "const MyClass = jest.fn();\nconst a = new MyClass();\nconst b = new MyClass();\nMyClass.mock.instances; // [a, b]"
    },
    {
      "id": "s10-54",
      "sectionId": "s10",
      "title": "Call Data",
      "kind": "code",
      "lang": "js",
      "code": "fn.mock.calls.length;\nfn.mock.calls[0][0];"
    },
    {
      "id": "s10-55",
      "sectionId": "s10",
      "title": "Return Values",
      "kind": "code",
      "lang": "js",
      "code": "jest.fn().mockReturnValue('hello');\njest.fn().mockReturnValueOnce('hi');"
    },
    {
      "id": "s10-56",
      "sectionId": "s10",
      "title": "Mock Implementations",
      "kind": "code",
      "lang": "js",
      "code": "const fn = jest\n  .fn()\n  .mockImplementationOnce(() => 1)\n  .mockImplementationOnce(() => 2);"
