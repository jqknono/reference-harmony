    {
      "id": "s2-27",
      "sectionId": "s2",
      "title": "Redirect output to file",
      "kind": "code",
      "lang": "bash",
      "code": "$ curl http://url/file > file"
    },
    {
      "id": "s2-28",
      "sectionId": "s2",
      "title": "Basic Authentication",
      "kind": "code",
      "lang": "bash",
      "code": "$ curl --user username:password http://example.com/\n$ curl -u username:password http://example.com/"
    },
    {
      "id": "s2-29",
      "sectionId": "s2",
      "title": "Write to file instead of stdout",
      "kind": "code",
      "lang": "bash",
      "code": "$ curl -o file http://url/file\n$ curl --output file http://url/file"
    },
    {
      "id": "s2-30",
      "sectionId": "s2",
      "title": "Download header information",
      "kind": "code",
      "lang": "bash",
      "code": "$ curl -I url\n# display header information"
    },
    {
      "id": "s2-31",
      "sectionId": "s2",
      "title": "Write output to a file named remote_file",
      "kind": "code",
      "lang": "bash",
      "code": "$ curl -o file http://url/file\n$ curl --output file http://url/file"
    },
    {
      "id": "s2-32",
      "sectionId": "s2",
      "title": "Execute remote script",
      "kind": "code",
      "lang": "bash",
      "code": "$ curl -s http://url/myscript.sh"
    },
    {
      "id": "s2-33",
      "sectionId": "s2",
      "title": "Configuration file",
      "kind": "code",
      "lang": "bash",
      "code": "curl -K file\n# read configuration from file\ncurl --config file\n$HOME/.curlrc # default configuration file on UNIX-like systems"
    }
