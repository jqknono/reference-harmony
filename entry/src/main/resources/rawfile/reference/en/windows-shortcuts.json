{
  "id": "windows-shortcuts",
  "title": "Create God Mode Folder",
  "sections": [
    {
      "id": "intro",
      "title": "Intro",
      "startIndex": 0
    }
  ],
  "cards": [
    {
      "id": "intro-0",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "text",
      "body": "Copy selected content (Ctrl + C can also abort commands):"
    },
    {
      "id": "intro-1",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "code",
      "lang": "",
      "code": "Ctrl + C"
    },
    {
      "id": "intro-2",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "text",
      "body": "Paste copied content:"
    },
    {
      "id": "intro-3",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "code",
      "lang": "",
      "code": "Ctrl + V"
    },
    {
      "id": "intro-4",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "text",
      "body": "Paste unformatted content:"
    },
    {
      "id": "intro-5",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "code",
      "lang": "",
      "code": "Ctrl + Shift + V"
    },
    {
      "id": "intro-6",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "text",
      "body": "Delete and copy selected content (good for moving text, files, etc.):"
    },
    {
      "id": "intro-7",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "code",
      "lang": "",
      "code": "Ctrl + X"
    },
    {
      "id": "intro-8",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "text",
      "body": "Select all available content in focus:"
    },
    {
      "id": "intro-9",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "code",
      "lang": "",
      "code": "Ctrl + A"
    },
    {
      "id": "intro-10",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "text",
      "body": "Undo the previous action:"
    },
    {
      "id": "intro-11",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "code",
      "lang": "",
      "code": "Ctrl + Z"
    },
    {
      "id": "intro-12",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "text",
      "body": "Redo the previous action:"
    },
    {
      "id": "intro-13",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "code",
      "lang": "",
      "code": "Ctrl + Y"
    },
    {
      "id": "intro-14",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "text",
      "body": "Save what you're working on:"
    },
    {
      "id": "intro-15",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "code",
      "lang": "",
      "code": "Ctrl + S"
    },
    {
      "id": "intro-16",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "text",
      "body": "Open a new file:"
    },
    {
      "id": "intro-17",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "code",
      "lang": "",
      "code": "Ctrl + O"
    },
    {
      "id": "intro-18",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "text",
      "body": "Open the print dialog box:"
    },
    {
      "id": "intro-19",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "code",
      "lang": "",
      "code": "Ctrl + P"
    },
    {
      "id": "intro-20",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "text",
      "body": "Open the search tool to find something on the page:"
    },
    {
      "id": "intro-21",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "code",
      "lang": "",
      "code": "Ctrl + F"
    },
    {
      "id": "intro-22",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "text",
      "body": "Refresh the contents on the screen (only F5 works in some cases):"
    },
    {
      "id": "intro-23",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "code",
      "lang": "",
      "code": "Ctrl + R"
    },
    {
      "id": "intro-24",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "text",
      "body": "Close the active program:"
    },
    {
      "id": "intro-25",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "code",
      "lang": "",
      "code": "Alt + F4"
    },
    {
      "id": "intro-26",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "text",
      "body": "Enter full-screen mode:"
    },
    {
      "id": "intro-27",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "code",
      "lang": "",
      "code": "F11"
    },
    {
      "id": "intro-28",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "text",
      "body": "Stop or close a prompt or process:"
    },
    {
      "id": "intro-29",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "code",
      "lang": "",
      "code": "Esc"
    },
    {
      "id": "intro-30",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "text",
      "body": "Lock your user account:"
    },
    {
      "id": "intro-31",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "code",
      "lang": "",
      "code": "Win + L"
    },
    {
      "id": "intro-32",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "text",
      "body": "Open File Explorer:"
    },
    {
      "id": "intro-33",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "code",
      "lang": "",
      "code": "Win + E"
    },
    {
      "id": "intro-34",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "text",
      "body": "Open Windows Settings:"
    },
    {
      "id": "intro-35",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "code",
      "lang": "",
      "code": "Win + i"
    },
    {
      "id": "intro-36",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "text",
      "body": "Move focus to the navigation bar to edit or copy the folder path:"
    },
    {
      "id": "intro-37",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "code",
      "lang": "",
      "code": "Alt + D"
    },
    {
      "id": "intro-38",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "text",
      "body": "Go to the folder the current folder is stored in:"
    },
    {
      "id": "intro-39",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "code",
      "lang": "",
      "code": "Alt + Up"
    },
    {
      "id": "intro-40",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "text",
      "body": "Show your password on the sign-in screen:"
    },
    {
      "id": "intro-41",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "code",
      "lang": "",
      "code": "Alt + F8"
    },
    {
      "id": "intro-42",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "text",
      "body": "Open Task Manager (Ctrl + Alt + Del works, too):"
    },
    {
      "id": "intro-43",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "code",
      "lang": "",
      "code": "Ctrl + Shift + Esc"
    },
    {
      "id": "intro-44",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "text",
      "body": "Open the Run dialog box to run commands:"
    },
    {
      "id": "intro-45",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "code",
      "lang": "",
      "code": "Win + R"
    },
    {
      "id": "intro-46",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "text",
      "body": "Quickly switch to the desktop:"
    },
    {
      "id": "intro-47",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "code",
      "lang": "",
      "code": "Win + D"
    },
    {
      "id": "intro-48",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "text",
      "body": "Open the selected item's Properties window:"
    },
    {
      "id": "intro-49",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "code",
      "lang": "",
      "code": "Alt + [Enter or double-click]"
    },
    {
      "id": "intro-50",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "text",
      "body": "Add a Virtual Desktop:"
    },
    {
      "id": "intro-51",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "code",
      "lang": "",
      "code": "Win + Ctrl + D"
    },
    {
      "id": "intro-52",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "text",
      "body": "Switch to the virtual desktop on the left or right:"
    },
    {
      "id": "intro-53",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "code",
      "lang": "",
      "code": "Win + Ctrl + [Left or Right]"
    },
    {
      "id": "intro-54",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "text",
      "body": "Show All Open Desktops:"
    },
    {
      "id": "intro-55",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "code",
      "lang": "",
      "code": "WIN + Tab"
    },
    {
      "id": "intro-56",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "text",
      "body": "Close Current Virtual Desktop:"
    },
    {
      "id": "intro-57",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "code",
      "lang": "",
      "code": "WIN + Ctrl + F4"
    },
    {
      "id": "intro-58",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "text",
      "body": "Select noncontiguous items (like files or folders):"
    },
    {
      "id": "intro-59",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "code",
      "lang": "",
      "code": "Ctrl + Click"
    },
    {
      "id": "intro-60",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "text",
      "body": "A possible fix for when your screen is black:"
    },
    {
      "id": "intro-61",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "code",
      "lang": "",
      "code": "Win + Ctrl + Shift + B"
    },
    {
      "id": "intro-62",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "text",
      "body": "Select every item between the first and last select items:"
    },
    {
      "id": "intro-63",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "code",
      "lang": "",
      "code": "Shift + Click"
    },
    {
      "id": "intro-64",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "text",
      "body": "Permanently delete a file or folder (it skips the Recycle Bin):"
    },
    {
      "id": "intro-65",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "code",
      "lang": "",
      "code": "Shift + Del"
    },
    {
      "id": "intro-66",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "text",
      "body": "Switch to the last used window:"
    },
    {
      "id": "intro-67",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "code",
      "lang": "",
      "code": "Alt + Tab"
    },
    {
      "id": "intro-68",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "text",
      "body": "Snap the active window to one side of the screen:"
    },
    {
      "id": "intro-69",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "code",
      "lang": "",
      "code": "Win + [Arrow]"
    },
    {
      "id": "intro-70",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "text",
      "body": "Rename the selected file or folder:"
    },
    {
      "id": "intro-71",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "code",
      "lang": "",
      "code": "F2"
    },
    {
      "id": "intro-72",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "text",
      "body": "Open the Power User Menu:"
    },
    {
      "id": "intro-73",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "code",
      "lang": "",
      "code": "Win + X"
    },
    {
      "id": "intro-74",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "text",
      "body": "Open a new instance of that app:"
    },
    {
      "id": "intro-75",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "code",
      "lang": "",
      "code": "Shift + Click Taskbar Icon"
    },
    {
      "id": "intro-76",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "text",
      "body": "Launch that item from the taskbar:"
    },
    {
      "id": "intro-77",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "code",
      "lang": "",
      "code": "Win + [number]"
    },
    {
      "id": "intro-78",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "text",
      "body": "Make a new folder:"
    },
    {
      "id": "intro-79",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "code",
      "lang": "",
      "code": "Ctrl + Shift + N"
    },
    {
      "id": "intro-80",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "text",
      "body": "Open the Windows About page:"
    },
    {
      "id": "intro-81",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "code",
      "lang": "",
      "code": "Win + Pause"
    },
    {
      "id": "intro-82",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "text",
      "body": "Screenshot just the active window:"
    },
    {
      "id": "intro-83",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "code",
      "lang": "",
      "code": "Alt + PrtScn"
    },
    {
      "id": "intro-84",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "text",
      "body": "Auto-save full-screen screenshot to Pictures > Screenshots:"
    },
    {
      "id": "intro-85",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "code",
      "lang": "",
      "code": "Win + PrtScn"
    },
    {
      "id": "intro-86",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "text",
      "body": "Choose a part of the screen to make a screenshot:"
    },
    {
      "id": "intro-87",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "code",
      "lang": "",
      "code": "Win + Shift + S"
    },
    {
      "id": "intro-88",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "text",
      "body": "Bold the selected text:"
    },
    {
      "id": "intro-89",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "code",
      "lang": "",
      "code": "Ctrl + B"
    },
    {
      "id": "intro-90",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "text",
      "body": "Italicize the selected text:"
    },
    {
      "id": "intro-91",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "code",
      "lang": "",
      "code": "Ctrl + i"
    },
    {
      "id": "intro-92",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "text",
      "body": "Underline the selected text:"
    },
    {
      "id": "intro-93",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "code",
      "lang": "",
      "code": "Ctrl + U"
    },
    {
      "id": "intro-94",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "text",
      "body": "Insert a hyperlink into the selected text:"
    },
    {
      "id": "intro-95",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "code",
      "lang": "",
      "code": "Ctrl + K"
    },
    {
      "id": "intro-96",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "text",
      "body": "Open the Find and Replace tool (confirmed in MS Word and Google Docs):"
    },
    {
      "id": "intro-97",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "code",
      "lang": "",
      "code": "Ctrl + H"
    },
    {
      "id": "intro-98",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "text",
      "body": "Pick an arrow key with this shortcut to quickly highlight a whole word or paragraph:"
    },
    {
      "id": "intro-99",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "code",
      "lang": "",
      "code": "Shift + Ctrl + [Arrow]"
    },
    {
      "id": "intro-100",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "text",
      "body": "Highlight everything from the cursor to the beginning or end of the line:"
    },
    {
      "id": "intro-101",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "code",
      "lang": "",
      "code": "Shift + [Home or End]"
    },
    {
      "id": "intro-102",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "text",
      "body": "Delete the word to the right of the cursor:"
    },
    {
      "id": "intro-103",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "code",
      "lang": "",
      "code": "Ctrl + Del"
    },
    {
      "id": "intro-104",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "text",
      "body": "Open a new tab:"
    },
    {
      "id": "intro-105",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "code",
      "lang": "",
      "code": "Ctrl + T"
    },
    {
      "id": "intro-106",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "text",
      "body": "Reopen the most recently closed tab:"
    },
    {
      "id": "intro-107",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "code",
      "lang": "",
      "code": "Ctrl + Shift + T"
    },
    {
      "id": "intro-108",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "text",
      "body": "Close the active tab:"
    },
    {
      "id": "intro-109",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "code",
      "lang": "",
      "code": "Ctrl + W"
    },
    {
      "id": "intro-110",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "text",
      "body": "Open the link in a new tab:"
    },
    {
      "id": "intro-111",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "code",
      "lang": "",
      "code": "Ctrl + [Link]"
    },
    {
      "id": "intro-112",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "text",
      "body": "View your web browsing history:"
    },
    {
      "id": "intro-113",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "code",
      "lang": "",
      "code": "Ctrl + H"
    },
    {
      "id": "intro-114",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "text",
      "body": "View recent or active downloads:"
    },
    {
      "id": "intro-115",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "code",
      "lang": "",
      "code": "Ctrl + J"
    },
    {
      "id": "intro-116",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "text",
      "body": "Start a search using the default search engine:"
    },
    {
      "id": "intro-117",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "code",
      "lang": "",
      "code": "Ctrl + E"
    },
    {
      "id": "intro-118",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "text",
      "body": "Jump to the tab in that position from the left (e.g., Ctrl + 4):"
    },
    {
      "id": "intro-119",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "code",
      "lang": "",
      "code": "Ctrl + [number]"
    },
    {
      "id": "intro-120",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "text",
      "body": "Open the options to delete browsing data:"
    },
    {
      "id": "intro-121",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "code",
      "lang": "",
      "code": "Ctrl + Shift + Del"
    },
    {
      "id": "intro-122",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "text",
      "body": "Go back or forward a page:"
    },
    {
      "id": "intro-123",
      "sectionId": "intro",
      "title": "Intro",
      "kind": "code",
      "lang": "",
