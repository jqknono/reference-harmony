      "id": "s1-26",
      "sectionId": "s1",
      "title": "Number",
      "kind": "qa",
      "front": "`Exponent`",
      "back": "Exponent like e, e+, e-, E, E+, E"
    },
    {
      "id": "s1-27",
      "sectionId": "s1",
      "title": "Number",
      "kind": "text",
      "body": "#### Examples"
    },
    {
      "id": "s1-28",
      "sectionId": "s1",
      "title": "Number",
      "kind": "code",
      "lang": "json",
      "code": "{\n  \"positive\": 12,\n  \"negative\": -1,\n  \"fraction\": 10.25,\n  \"exponent\": 1.0e2,\n  \"zero\": 0\n}"
    },
    {
      "id": "s1-29",
      "sectionId": "s1",
      "title": "Number",
      "kind": "text",
      "body": "#### Invalid Number"
    },
    {
      "id": "s1-30",
      "sectionId": "s1",
      "title": "Number",
      "kind": "code",
      "lang": "json",
      "code": "{ \"foo\": 0xff }"
    },
    {
      "id": "s1-31",
      "sectionId": "s1",
      "title": "Number",
      "kind": "text",
      "body": "In JSON you can use only Decimal Literals"
    },
    {
      "id": "s1-32",
      "sectionId": "s1",
      "title": "Objects",
      "kind": "code",
      "lang": "json",
      "code": "{\n  \"color\": \"Purple\",\n  \"id\": \"210\",\n  \"composition\": {\n    \"R\": 70,\n    \"G\": 39,\n    \"B\": 89\n  },\n  \"empty_object\": {}\n}"
    },
    {
      "id": "s1-33",
      "sectionId": "s1",
      "title": "Objects",
      "kind": "text",
      "body": "Multiple key/value pairs separated by a comma"
    },
