    {
      "id": "s1-0",
      "sectionId": "s1",
      "title": "Basic Usage :",
      "kind": "code",
      "lang": "bash",
      "code": "# set your target in etc/hosts\n\n# fast scan of all port of your target\nnmap -p- -T5 target \n\n# fast scan on port 80 with os detection  \nnamp -p, 80 -T5 -O target"
    },
    {
      "id": "s1-1",
      "sectionId": "s1",
      "title": "Basic Options :",
      "kind": "qa",
      "front": "`-p-`",
      "back": "all ports"
    },
    {
      "id": "s1-2",
      "sectionId": "s1",
      "title": "Basic Options :",
      "kind": "qa",
      "front": "`-p1-1023`",
      "back": "scan ports 1 to 1023"
    },
    {
      "id": "s1-3",
      "sectionId": "s1",
      "title": "Basic Options :",
      "kind": "qa",
      "front": "`-F`",
      "back": "100 most common ports"
    },
    {
      "id": "s1-4",
      "sectionId": "s1",
      "title": "Basic Options :",
      "kind": "qa",
      "front": "`-r`",
      "back": "scan ports in consecutive order"
    },
    {
      "id": "s1-5",
      "sectionId": "s1",
      "title": "Basic Options :",
      "kind": "qa",
      "front": "`-T<0-5>`",
      "back": "-T0 being the slowest and T5 the fastest"
    },
    {
      "id": "s1-6",
      "sectionId": "s1",
      "title": "Basic Options :",
      "kind": "qa",
      "front": "`--max-rate 50`",
      "back": "rate <= 50 packets/sec"
    },
    {
      "id": "s1-7",
      "sectionId": "s1",
      "title": "Basic Options :",
      "kind": "qa",
      "front": "`--min-rate 15`",
      "back": "rate >= 15 packets/sec"
    },
    {
      "id": "s1-8",
      "sectionId": "s1",
      "title": "Basic Options :",
      "kind": "qa",
      "front": "`--min-parallelism 100`",
      "back": "at least 100 probes in parallel"
    },
    {
      "id": "s2-9",
      "sectionId": "s2",
      "title": "Service Detections :",
      "kind": "qa",
      "front": "`--traceroute`",
      "back": "run traceroute to target"
    },
    {
      "id": "s2-10",
      "sectionId": "s2",
      "title": "Service Detections :",
      "kind": "qa",
      "front": "`--script=SCRIPTS`",
      "back": "Nmap scripts to run"
    },
    {
      "id": "s2-11",
      "sectionId": "s2",
      "title": "Service Detections :",
      "kind": "qa",
      "front": "`-sC` or `--script=default`",
      "back": "run default scripts"
    },
    {
      "id": "s2-12",
      "sectionId": "s2",
      "title": "Service Detections :",
      "kind": "qa",
      "front": "`-O`",
      "back": "OS detection"
    },
    {
      "id": "s2-13",
      "sectionId": "s2",
      "title": "Service Detections :",
      "kind": "qa",
      "front": "`-sV`",
      "back": "Service version detection"
    },
    {
      "id": "s2-14",
      "sectionId": "s2",
      "title": "Service Detections :",
      "kind": "qa",
      "front": "`-A`",
      "back": "equivalent to `-sV -O -sC --traceroute`"
