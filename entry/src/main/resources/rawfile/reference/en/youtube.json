      "title": "Intro",
      "kind": "text",
      "body": "A visual cheat-sheet for the 18 keyboard shortcuts found on YouTube.com"
    },
    {
      "id": "s1-1",
      "sectionId": "s1",
      "title": "General I",
      "kind": "qa",
      "front": "`Space`",
      "back": "Play/Pause when the seek bar is selected, activate a button if a button has focus"
    },
    {
      "id": "s1-2",
      "sectionId": "s1",
      "title": "General I",
      "kind": "qa",
      "front": "`K`",
      "back": "Play/Pause in player"
    },
    {
      "id": "s1-3",
      "sectionId": "s1",
      "title": "General I",
      "kind": "qa",
      "front": "`Left/Right`",
      "back": "Seek backward/forward 5 seconds"
    },
    {
      "id": "s1-4",
      "sectionId": "s1",
      "title": "General I",
      "kind": "qa",
      "front": "`J`",
      "back": "Seek backward 10 seconds in player"
    },
    {
      "id": "s1-5",
      "sectionId": "s1",
      "title": "General I",
      "kind": "qa",
      "front": "`L`",
      "back": "Seek forward 10 seconds in player"
    },
    {
      "id": "s1-6",
      "sectionId": "s1",
      "title": "General I",
      "kind": "qa",
      "front": "`Home`",
      "back": "Seek to the beginning of the video"
    },
    {
      "id": "s1-7",
      "sectionId": "s1",
      "title": "General I",
      "kind": "qa",
      "front": "`End`",
      "back": "Seek to the last seconds of the video"
    },
    {
      "id": "s1-8",
      "sectionId": "s1",
      "title": "General I",
      "kind": "qa",
      "front": "`Up`",
      "back": "Increase volume 5%"
    },
    {
      "id": "s1-9",
      "sectionId": "s1",
      "title": "General I",
      "kind": "qa",
      "front": "`Down`",
      "back": "Decrease volume 5%"
    },
    {
      "id": "s1-10",
      "sectionId": "s1",
      "title": "General I",
      "kind": "qa",
      "front": "`1-9`",
      "back": "Seek to the 10-90% of the video"
    },
    {
      "id": "s1-11",
      "sectionId": "s1",
      "title": "General II",
      "kind": "qa",
      "front": "`0`",
      "back": "Seek to the beginning of the video"
    },
    {
      "id": "s1-12",
      "sectionId": "s1",
      "title": "General II",
      "kind": "qa",
      "front": "`Shift` `1`",
      "back": "Move between H1 headers"
    },
    {
      "id": "s1-13",
      "sectionId": "s1",
      "title": "General II",
      "kind": "qa",
      "front": "`/`",
      "back": "Go to search box"
    },
    {
      "id": "s1-14",
      "sectionId": "s1",
      "title": "General II",
      "kind": "qa",
      "front": "`F`",
      "back": "Activate full screen, press again (or Esc) to exit"
