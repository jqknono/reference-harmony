    {
      "id": "s1-6",
      "sectionId": "s1",
      "title": "Strings",
      "body": "%60%60%60php%0A%24url%20%3D%20%22cheatsheets.zip%22%3B%0Aecho%20%22I'm%20learning%20PHP%20at%20%24url%22%3B%0A%0A%2F%2F%20Concatenate%20strings%0Aecho%20%22I'm%20learning%20PHP%20at%20%22%20.%20%24url%3B%0A%0A%24hello%20%3D%20%22Hello%2C%20%22%3B%0A%24hello%20.%3D%20%22World!%22%3B%0Aecho%20%24hello%3B%20%20%20%23%20%3D%3E%20Hello%2C%20World!%0A%60%60%60%0A%0ASee%3A%20%5BStrings%5D(%23php-strings)",
      "encoded": true
