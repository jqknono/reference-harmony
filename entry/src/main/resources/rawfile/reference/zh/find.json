      "code": "%24%20find%20.%20-type%20f%20%5C(%20-name%20%22*.sh%22%20-or%20-name%20%22*.txt%22%20%5C)",
      "encoded": true
    },
    {
      "id": "s1-48",
      "sectionId": "s1",
      "title": "",
      "kind": "text",
      "body": "查找带有 `.sh` 或 `.txt` 扩展名的文件"
    },
    {
      "id": "s1-49",
      "sectionId": "s1",
      "title": "多个目录",
      "kind": "code",
      "lang": "shell",
      "code": "$ find /opt /usr /var -name foo.scala -type f"
    },
    {
      "id": "s1-50",
      "sectionId": "s1",
      "title": "",
      "kind": "text",
      "body": "查找具有多个目录的文件"
    },
