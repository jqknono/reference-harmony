      "id": "s1-32",
      "sectionId": "s1",
      "title": "",
      "kind": "code",
      "lang": "shell",
      "code": "$ find / -perm /u=s"
    },
    {
      "id": "s1-33",
      "sectionId": "s1",
      "title": "",
      "kind": "text",
      "body": "查找 SGID 集文件。"
    },
    {
      "id": "s1-34",
      "sectionId": "s1",
      "title": "",
      "kind": "code",
      "lang": "shell",
      "code": "$ find / -perm /g=s"
    },
    {
      "id": "s1-35",
      "sectionId": "s1",
      "title": "",
      "kind": "text",
      "body": "查找只读文件。"
    },
    {
      "id": "s1-36",
      "sectionId": "s1",
      "title": "",
      "kind": "code",
      "lang": "shell",
      "code": "$ find / -perm /u=r"
    },
    {
      "id": "s1-37",
      "sectionId": "s1",
      "title": "",
      "kind": "text",
      "body": "查找可执行文件。"
    },
    {
      "id": "s1-38",
      "sectionId": "s1",
      "title": "",
      "kind": "code",
      "lang": "shell",
      "code": "$ find / -perm /a=x"
    },
    {
      "id": "s1-39",
      "sectionId": "s1",
      "title": "所有者和组",
