      "id": "s1-24",
      "sectionId": "s1",
      "title": "",
      "kind": "code",
      "lang": "shell",
      "code": "$ find . -type f -name tecmint.php"
    },
    {
      "id": "s1-25",
      "sectionId": "s1",
      "title": "",
      "kind": "text",
      "body": "查找目录下的所有php文件"
    },
