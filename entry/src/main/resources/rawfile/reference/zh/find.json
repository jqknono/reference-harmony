      "id": "s2-65",
      "sectionId": "s2",
      "title": "",
      "kind": "code",
      "lang": "shell",
      "code": "$ find / -mmin -60"
    },
    {
      "id": "s2-66",
      "sectionId": "s2",
      "title": "",
      "kind": "text",
      "body": "查找最近 1 小时内访问过的文件"
    },
    {
      "id": "s2-67",
      "sectionId": "s2",
      "title": "",
      "kind": "code",
      "lang": "shell",
      "code": "$ find / -amin -60"
    },
    {
      "id": "s3-68",
      "sectionId": "s3",
      "title": "查找和删除",
      "kind": "text",
      "body": "查找并删除多个文件"
