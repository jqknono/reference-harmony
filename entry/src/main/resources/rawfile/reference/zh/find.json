    {
      "id": "s1-5",
      "sectionId": "s1",
      "title": "%E5%8F%82%E6%95%B0%E7%A4%BA%E4%BE%8B",
      "body": "%7C%20%E5%8F%82%E6%95%B0%20%7C%20%E7%A4%BA%E4%BE%8B%20%7C%20%E6%8F%8F%E8%BF%B0%20%7C%0A%7C-------%7C-------%7C-------%7C%0A%7C%20%60-type%60%20%20%20%20%20%7C%20find%20.%20-type%20d%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%7C%20%E4%BB%85%E6%9F%A5%E6%89%BE%E7%9B%AE%E5%BD%95%20%7C%0A%7C%20%60-name%60%20%20%20%20%20%7C%20find%20.%20-type%20f%20-name%20%22*.txt%22%20%20%20%20%20%20%20%20%20%20%20%20%20%20%7C%20%E6%8C%89%E5%90%8D%E7%A7%B0%E6%9F%A5%E6%89%BE%E6%96%87%E4%BB%B6%20%7C%0A%7C%20%60-iname%60%20%20%20%20%7C%20find%20.%20-type%20f%20-iname%20%22hello%22%20%20%20%20%20%20%20%20%20%20%20%20%20%7C%20%E6%8C%89%E5%90%8D%E7%A7%B0%E6%9F%A5%E6%89%BE%E6%96%87%E4%BB%B6%EF%BC%88%E4%B8%8D%E5%8C%BA%E5%88%86%E5%A4%A7%E5%B0%8F%E5%86%99%EF%BC%89%20%7C%0A%7C%20%60-size%60%20%20%20%20%20%7C%20find%20.%20-size%20%2B1G%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%7C%20%E6%9F%A5%E6%89%BE%E5%A4%A7%E4%BA%8E%201G%20%E7%9A%84%E6%96%87%E4%BB%B6%20%7C%0A%7C%20%60-user%60%20%20%20%20%20%7C%20find%20.%20-type%20d%20-user%20jack%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%7C%20%E6%9F%A5%E6%89%BE%E6%9D%B0%E5%85%8B%E7%9A%84%E6%96%87%E4%BB%B6%20%7C%0A%7C%20%60-regex%60%20%20%20%20%7C%20find%20%2Fvar%20-regex%20'.%5C*%2Ftmp%2F.%5C*%5B0-9%5D*.file'%20%7C%20%E5%B0%86%E6%AD%A3%E5%88%99%E8%A1%A8%E8%BE%BE%E5%BC%8F%E4%B8%8E%E6%9F%A5%E6%89%BE%E4%B8%80%E8%B5%B7%E4%BD%BF%E7%94%A8%20%7C%0A%7C%20%60-maxdepth%60%20%7C%20find%20.%20-maxdepth%201%20-name%20%22a.txt%22%20%20%20%20%20%20%20%20%20%20%7C%20%E5%9C%A8%E5%BD%93%E5%89%8D%E7%9B%AE%E5%BD%95%E5%92%8C%E5%AD%90%E7%9B%AE%E5%BD%95%E4%B8%AD%20%7C%0A%7C%20%60-mindepth%60%20%7C%20find%20%2F%20-mindepth%203%20-maxdepth%205%20-name%20pass%20%7C%20%E5%9C%A8%E5%AD%90%E7%9B%AE%E5%BD%95%E7%BA%A7%E5%88%AB%202%20%E5%92%8C%204%20%E4%B9%8B%E9%97%B4%20%7C",
