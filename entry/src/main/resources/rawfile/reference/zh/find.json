      "sectionId": "s3",
      "title": "",
      "kind": "code",
      "lang": "shell",
      "code": "%24%20find%20.%20-type%20f%20-name%20%22tecmint.txt%22%20-exec%20rm%20-f%20%7B%7D%20%5C%3B",
