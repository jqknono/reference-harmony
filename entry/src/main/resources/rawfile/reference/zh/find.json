      "id": "s1-40",
      "sectionId": "s1",
      "title": "",
      "kind": "code",
      "lang": "shell",
      "code": "$ find / -user root -name tecmint.txt"
    },
    {
      "id": "s1-41",
      "sectionId": "s1",
      "title": "",
      "kind": "text",
      "body": "根据用户查找所有文件"
    },
    {
      "id": "s1-42",
      "sectionId": "s1",
      "title": "",
      "kind": "code",
      "lang": "shell",
      "code": "$ find /home -user tecmint"
    },
    {
      "id": "s1-43",
      "sectionId": "s1",
      "title": "",
      "kind": "text",
      "body": "根据组查找所有文件"
    },
    {
      "id": "s1-44",
      "sectionId": "s1",
      "title": "",
      "kind": "code",
      "lang": "shell",
      "code": "$ find /home -group developer"
    },
    {
      "id": "s1-45",
      "sectionId": "s1",
      "title": "",
      "kind": "text",
      "body": "查找用户的特定文件"
    },
    {
      "id": "s1-46",
      "sectionId": "s1",
      "title": "",
      "kind": "code",
      "lang": "shell",
      "code": "%24%20find%20%2Fhome%20-user%20tecmint%20-iname%20%22*.txt%22",
