      "code": "{\n  \"Mark McGwire\": {\n    \"hr\": 65,\n    \"avg\": 0.278\n  },\n  \"Sammy Sosa\": {\n    \"hr\": 63,\n    \"avg\": 0.288\n  }\n}"
    },
    {
      "id": "intro-46",
      "sectionId": "intro",
      "title": "嵌套集合",
      "kind": "code",
      "lang": "yaml",
      "code": "Jack:\n  id: 1\n  name: Franc\n  salary: 25000\n  hobby:\n    - a\n    - b\n  location: {country: \"A\", city: \"A-A\"}"
    },
    {
      "id": "intro-47",
      "sectionId": "intro",
      "title": "嵌套集合",
