      "sectionId": "intro",
      "title": "序列",
      "kind": "text",
      "body": "#### ↓ 等效的 JSON"
    },
    {
      "id": "intro-30",
      "sectionId": "intro",
      "title": "序列",
      "kind": "code",
      "lang": "json",
      "code": "[\n  \"Mark McGwire\",\n  \"Sammy Sosa\",\n  \"Ken Griffey\"\n]"
    },
    {
      "id": "intro-31",
      "sectionId": "intro",
      "title": "映射",
      "kind": "code",
      "lang": "yaml",
      "code": "hr:  65       # Home runs\navg: 0.278    # Batting average\nrbi: 147      # Runs Batted In"
    },
    {
      "id": "intro-32",
      "sectionId": "intro",
      "title": "映射",
      "kind": "text",
      "body": "#### ↓ 等效的 JSON"
    },
    {
      "id": "intro-33",
      "sectionId": "intro",
      "title": "映射",
      "kind": "code",
      "lang": "json",
      "code": "{\n  \"hr\": 65,\n  \"avg\": 0.278,\n  \"rbi\": 147\n}"
    },
    {
      "id": "intro-34",
      "sectionId": "intro",
      "title": "映射到序列",
      "kind": "code",
      "lang": "yaml",
      "code": "attributes:\n  - a1\n  - a2\nmethods: [getter, setter]"
    },
    {
      "id": "intro-35",
      "sectionId": "intro",
      "title": "映射到序列",
      "kind": "text",
      "body": "#### ↓ 等效的 JSON"
    },
    {
      "id": "intro-36",
      "sectionId": "intro",
      "title": "映射到序列",
      "kind": "code",
      "lang": "json",
      "code": "{\n  \"attributes\": [\"a1\", \"a2\"],\n  \"methods\": [\"getter\", \"setter\"]\n}"
    },
    {
      "id": "intro-37",
      "sectionId": "intro",
      "title": "映射序列",
      "kind": "code",
      "lang": "yaml",
      "code": "children:\n  - name: Jimmy Smith\n    age: 15\n  - name: Jimmy Smith\n    age: 15\n  -\n    name: Sammy Sosa\n    age: 12"
    },
    {
      "id": "intro-38",
      "sectionId": "intro",
      "title": "映射序列",
      "kind": "text",
      "body": "#### ↓ 等效的 JSON"
    },
    {
