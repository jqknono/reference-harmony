      "body": "%7C%20%E5%87%BD%E6%95%B0%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%7C%20%E8%A7%A3%E9%87%8A%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%7C%20%E7%A4%BA%E4%BE%8B%E8%AF%AD%E6%B3%95%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%7C%20%E7%BB%93%E6%9E%9C%20%20%20%20%20%20%20%20%20%20%20%20%7C%0A%7C%20%3A------------------------------%20%7C%20%3A------------------------%20%7C%20----------------------------%20%7C%20---------------%20%7C%0A%7C%20%60CONCAT(s1%2Cs2%2C...)%60%20%20%20%20%20%20%20%20%20%20%20%20%7C%20%E8%BF%9E%E6%8E%A5%E4%B8%A4%E4%B8%AA%E6%88%96%E6%9B%B4%E5%A4%9A%E5%AD%97%E7%AC%A6%E4%B8%B2%20%20%20%20%20%7C%20%60CONCAT('Hello%2C%20'%2C'World!')%60%20%7C%20'Hello%2C%20World!'%20%7C%0A%7C%20%60LOWER(str)%60%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%7C%20%E8%BD%AC%E6%8D%A2%E4%B8%BA%E5%B0%8F%E5%86%99%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%7C%20%60LOWER('HELLO')%60%20%20%20%20%20%20%20%20%20%20%20%20%20%7C%20'hello'%20%20%20%20%20%20%20%20%20%7C%0A%7C%20%60UPPER(str)%60%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%7C%20%E8%BD%AC%E6%8D%A2%E4%B8%BA%E5%A4%A7%E5%86%99%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%7C%20%60UPPER('world')%60%20%20%20%20%20%20%20%20%20%20%20%20%20%7C%20'WORLD'%20%20%20%20%20%20%20%20%20%7C%0A%7C%20%60TRIM(str)%60%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%7C%20%E5%8E%BB%E9%99%A4%E5%AD%97%E7%AC%A6%E4%B8%B2%E4%B8%A4%E7%AB%AF%E7%A9%BA%E6%A0%BC%20%20%20%20%20%20%20%7C%20%60TRIM('%20%20Hello%20%20')%60%20%20%20%20%20%20%20%20%20%20%7C%20'Hello'%20%20%20%20%20%20%20%20%20%7C%0A%7C%20%60LEFT(str%2Clen)%60%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%7C%20%E6%8F%90%E5%8F%96%E5%AD%97%E7%AC%A6%E4%B8%B2%E5%B7%A6%E4%BE%A7%E7%9A%84%E8%8B%A5%E5%B9%B2%E5%AD%97%E7%AC%A6%20%7C%20%60LEFT('Hello'%2C%203)%60%20%20%20%20%20%20%20%20%20%20%20%7C%20'Hel'%20%20%20%20%20%20%20%20%20%20%20%7C%0A%7C%20%60RIGHT(str%2Clen)%60%20%20%