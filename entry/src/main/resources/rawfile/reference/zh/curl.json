    {
      "id": "s2-26",
      "sectionId": "s2",
      "title": "将输出重定向到文件",
      "body": "```bash\n$ curl http://url/file > file\n```"
    },
    {
      "id": "s2-27",
      "sectionId": "s2",
      "title": "基本认证",
      "body": "```bash\n$ curl --user username:password http://example.com/\n$ curl -u username:password http://example.com/\n```"
    },
    {
      "id": "s2-28",
      "sectionId": "s2",
      "title": "写入文件而不是标准输出",
      "body": "```bash\n$ curl -o file http://url/file\n$ curl --output file http://url/file\n```"
    },
    {
      "id": "s2-29",
      "sectionId": "s2",
      "title": "下载头信息",
      "body": "```bash\n$ curl -I url\n# 显示头信息\n```"
    },
    {
      "id": "s2-30",
      "sectionId": "s2",
      "title": "将输出写入名为远程文件的文件",
      "body": "```bash\n$ curl -o file http://url/file\n$ curl --output file http://url/file\n```"
    },
    {
      "id": "s2-31",
      "sectionId": "s2",
      "title": "执行远程脚本",
      "body": "```bash\n$ curl -s http://url/myscript.sh\n```"
    },
    {
      "id": "s2-32",
      "sectionId": "s2",
      "title": "配置文件",
      "body": "```bash\ncurl -K file\n# 从文件中读取配置\ncurl --config file\n$HOME/.curlrc # 类 UNIX 系统中的默认配置文件\n```"
    }
