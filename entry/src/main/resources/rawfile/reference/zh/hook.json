      "id": "s3-32",
      "sectionId": "s3",
      "title": "%E4%BB%A3%E7%A0%81%E5%9D%97",
      "kind": "code",
      "lang": "js",
      "code": "%7B%0A%20%20println(%22Hello%22)%3B%0A%20%20%7B%0A%20%20%20%20println(%22World%22)%3B%0A%20%20%7D%0A%7D",
